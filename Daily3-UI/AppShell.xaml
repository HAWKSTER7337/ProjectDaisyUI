<?xml version="1.0" encoding="UTF-8"?>

<Shell
    x:Class="Daily3_UI.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Daily3_UI.Pages"
    xmlns:pagesDaily3="clr-namespace:Daily3_UI.Pages.PagesDaily3"
    xmlns:pagesDaily4="clr-namespace:Daily3_UI.Pages.PagesDaily4"
    FlyoutBehavior="Disabled">
    <ShellContent Route="LoginPage" ContentTemplate="{DataTemplate local:LogInPage}" />
    <ShellContent Route="Buy3" ContentTemplate="{DataTemplate pagesDaily3:BuyTickets}"/>
    <ShellContent Route="Buy4" ContentTemplate="{DataTemplate pagesDaily4:BuyTicketsDaily4}"/>
</Shell>